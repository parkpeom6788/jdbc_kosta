package com.model.dao;

import java.sql.Date;

public class Employee {
	private int empNo;
	private String empName;
	private String job;
	private int mgr;
	private Date hireDate;
	private int sql;
	private int comm;
	private int deptNo;
	
	public Employee() {
	}
	public Employee(int empNo, String job, int sql, int comm) {
		super();
		this.empNo = empNo;
		this.job = job;
		this.sql = sql;
		this.comm = comm;
	}
	public Employee(int empNo, String empName, String job, int mgr, Date hireDate, int sql, int comm, int deptNo) {
		super();
		this.empNo = empNo;
		this.empName = empName;
		this.job = job;
		this.mgr = mgr;
		this.hireDate = hireDate;
		this.sql = sql;
		this.comm = comm;
		this.deptNo = deptNo;
	}
	
}
